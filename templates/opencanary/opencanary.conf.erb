{
    "device.node_id": "<%= node[:fqdn] %>",
    "ftp.banner": "<%= node[:t1000][:opencanary][:ftp][:banner] %>",
    "ftp.enabled": <%= node[:t1000][:opencanary][:ftp][:enabled] %>,
    "ftp.port": <%= node[:t1000][:opencanary][:ftp][:port] %>,
    "http.banner": "<%= node[:t1000][:opencanary][:http][:banner] %>",
    "http.enabled": <%= node[:t1000][:opencanary][:http][:enabled] %>,
    "http.port": <%= node[:t1000][:opencanary][:http][:port] %>,
    "http.skin": "<%= node[:t1000][:opencanary][:http][:skin] %>",
    "http.skin.list": [
<%= @http_skins %>
    ],
    "httpproxy.enabled": <%= node[:t1000][:opencanary][:httpproxy][:enabled] %>,
    "httpproxy.port": <%= node[:t1000][:opencanary][:httpproxy][:port] %>,
    "httpproxy.skin": "<%= node[:t1000][:opencanary][:httpproxy][:skin] %>",
    "httproxy.skin.list": [
<%= @httpproxy_skins %>
    ],
    "logger": {
        "class": "PyLogger",
        "kwargs": {
            "formatters": {
                "plain": {
                    "format": "%(message)s"
                }
            },
            "handlers": {
                "console": {
                    "class": "logging.StreamHandler",
                    "stream": "ext://sys.stdout"
                },
                "file": {
                    "class": "logging.FileHandler",
                    "filename": "<%= node[:t1000][:opencanary][:logger][:file][:path] %>"
                },
                "syslog-unix": {
                    "class": "logging.handlers.SysLogHandler",
                    "address": [
                        "localhost",
                        514
                    ],
                    "socktype": "ext://socket.SOCK_DGRAM"
                },
                "json-tcp": {
                    "class": "opencanary.logger.SocketJSONHandler",
                    "host": "127.0.0.1",
                    "port": 1514
                }
            }
        }
    },
    "portscan.synrate": <%= node[:t1000][:opencanary][:portscan][:synrate] %>,
    "smb.auditfile": "<%= node[:t1000][:smb][:syslog_path] %>",
    "smb.configfile": "<%= node[:t1000][:opencanary][:smb][:configfile] %>",
    "smb.domain": "<%= node[:t1000][:opencanary][:smb][:domain] %>",
    "smb.enabled": <%= node[:t1000][:opencanary][:smb][:enabled] %>,
    "smb.filelist": [
<%= @smb_files %>
    ],
    "smb.mode": "<%= node[:t1000][:opencanary][:smb][:mode] %>",
    "smb.netbiosname": "<%= node[:t1000][:smb][:netbios_name] %>",
    "smb.serverstring": "<%= node[:t1000][:smb][:server_string] %>",
    "smb.sharecomment": "<%= node[:t1000][:smb][:share_comment] %>",
    "smb.sharename": "<%= node[:t1000][:smb][:share_name] %>",
    "smb.sharepath": "<%= node[:t1000][:smb][:share_path] %>",
    "smb.workgroup": "<%= node[:t1000][:smb][:workgroup] %>",
    "mysql.banner": "<%= node[:t1000][:opencanary][:mysql][:banner] %>",
    "mysql.port": <%= node[:t1000][:opencanary][:mysql][:port] %>,
    "mysql.enabled": <%= node[:t1000][:opencanary][:mysql][:enabled] %>,
    "ssh.enabled": <%= node[:t1000][:opencanary][:ssh][:enabled] %>,
    "ssh.port": <%= node[:t1000][:opencanary][:ssh][:port] %>,
    "ssh.version": "<%= node[:t1000][:opencanary][:ssh][:version] %>",
    "rdp.enabled": <%= node[:t1000][:opencanary][:rdp][:enabled] %>,
    "sip.enabled": <%= node[:t1000][:opencanary][:sip][:enabled] %>,
    "snmp.enabled": <%= node[:t1000][:opencanary][:snmp][:enabled] %>,
    "ntp.enabled": <%= node[:t1000][:opencanary][:ntp][:enabled] %>,
    "tftp.enabled": <%= node[:t1000][:opencanary][:tftp][:enabled] %>,
    "ntp.port": "<%= node[:t1000][:opencanary][:ntp][:port] %>",
    "telnet.port": "<%= node[:t1000][:opencanary][:telnet][:port] %>",
    "telnet.enabled": <%= node[:t1000][:opencanary][:telnet][:enabled] %>,
    "telnet.banner": "<%= node[:t1000][:opencanary][:telnet][:banner] %>",
    "telnet.honeycreds": [
<%= @telnet_honeycreds %>
    ],
    "mssql.enabled": <%= node[:t1000][:opencanary][:mssql][:enabled] %>,
    "vnc.enabled": <%= node[:t1000][:opencanary][:vnc][:enabled] %>
}
