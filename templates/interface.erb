#
# /etc/network/interfaces.d/<%= node[:t1000][:mgmt][:interface] %>
#
# Generated by Chef for <%= node[:fqdn] %>
# 

auto <%= node[:t1000][:mgmt][:interface] %>
iface <%= node[:t1000][:mgmt][:interface] %> inet static
  address <%= node[:t1000][:mgmt][:ipv4] %>
  gateway <%= node[:t1000][:mgmt][:gateway] %>
  netmask <%= node[:t1000][:mgmt][:netmask] %>
  dns-nameservers <%= node[:t1000][:mgmt][:nameserver] %>
  domain <%= node[:t1000][:mgmt][:domain] %>
